{
  "name": "warmd",
  "description": "WRCT: A Radio Music Database",
  "version": "0.0.1",
  "private": true,
  "contributors": [
    {
      "name": "Marcus Todd",
      "email": "malexander.todd@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/malexandert/warmd.git"
  },
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/grunt start",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --reporter spec --timeout 10000 tests/*-tests.js",
    "mongo-test": "./node_modules/.bin/mocha --reporter spec --timeout 10000 tests/mongo/",
    "deploy": "sudo NODE_ENV=production ./node_modules/forever/bin/forever start -a -l forever.log -o output.log -e errors.log server.js",
    "halt": "NODE_ENV=production ./node_modules/forever/bin/forever stop server.js",
    "postinstall": "./scripts/postinstall.sh",
    "lint": "eslint",
    "lint-all": "eslint app test"
  },
  "engines": {
    "node": "~0.10.22",
    "npm": "^1.4.3"
  },
  "jshintConfig": {
    "node": true,
    "curly": true,
    "indent": 2,
    "newcap": true,
    "strict": true,
    "trailing": true
  },
  "dependencies": {
    "acl": "~0.4.0",
    "body-parser": "~1.6.0",
    "bookshelf": "~0.7.0",
    "checkit": "^0.2.0-pre",
    "cookie-parser": "~1.3.0",
    "express": "~4.0.0",
    "express-hbs": "~0.7.8",
    "express-session": "~1.7.0",
    "forever": "^0.11.0",
    "knex": "~0.6.0",
    "lodash": "~2.4.1",
    "method-override": "~1.0.0",
    "mongoose": "^4.13.6",
    "morgan": "~1.1.1",
    "mysql": "~2.3.0",
    "nodemailer": "~1.1.0",
    "passport": "~0.2.0",
    "passport-local": "~1.0.0",
    "request": "~2.36.0",
    "request-json": "~0.4.10",
    "winston": "~0.7.3",
    "winston-mail": "~0.2.7"
  },
  "devDependencies": {
    "eslint": "^4.10.0",
    "grunt": "~0.4.4",
    "grunt-cli": "~0.1.13",
    "grunt-concurrent": "~0.5.0",
    "grunt-contrib-jshint": "~0.10.0",
    "grunt-node-inspector": "^0.4.1",
    "grunt-nodemon": "~0.2.0",
    "grunt-shell": "~0.6.4",
    "load-grunt-tasks": "~0.4.0",
    "mocha": "^1.18.2",
    "nodemon": "~1.0.17",
    "should": "^3.3.1",
    "supertest": "^0.13.0"
  }
}
