{
  "name": "warmd",
  "description": "WRCT: A Radio Music Database",
  "version": "0.0.1",
  "private": true,
  "contributors": [
    {
      "name": "Marcus Todd",
      "email": "malexander.todd@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/wrct883/warmd.git"
  },
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=development ./node_modules/.bin/nodemon server.js",
    "test": "NPM_CONFIG_MONGO=true NODE_ENV=test ./node_modules/.bin/mocha --reporter spec --timeout 10000 tests/integration/ tests/unit/ --exit",
    "deploy": "sudo NODE_ENV=production ./node_modules/forever/bin/forever start -a -l forever.log -o output.log -e errors.log server.js",
    "halt": "NODE_ENV=production ./node_modules/forever/bin/forever stop server.js",
    "postinstall": "./scripts/postinstall.sh",
    "lint": "eslint",
    "lint-all": "eslint app test"
  },
  "engines": {
    "node": "~0.10.22",
    "npm": "^1.4.3"
  },
  "jshintConfig": {
    "node": true,
    "curly": true,
    "indent": 2,
    "newcap": true,
    "strict": true,
    "trailing": true
  },
  "dependencies": {
    "acl": "~0.4.0",
    "bcrypt": "^1.0.3",
    "body-parser": "~1.6.0",
    "bookshelf": "^0.13.3",
    "checkit": "^0.2.0-pre",
    "cookie-parser": "~1.3.0",
    "express": "^4.16.3",
    "express-hbs": "^1.0.4",
    "express-session": "^1.15.6",
    "forever": "^0.15.3",
    "knex": "^0.15.2",
    "lodash": "^4.17.11",
    "method-override": "^3.0.0",
    "mocha": "^4.0.1",
    "mongoose": "^4.13.6",
    "mongoose-hidden": "^1.5.1",
    "morgan": "~1.1.1",
    "mysql": "^2.16.0",
    "nodemailer": "~1.1.0",
    "number-to-words": "^1.2.3",
    "passport": "~0.2.0",
    "passport-local": "~1.0.0",
    "request": "^2.88.0",
    "request-json": "^0.6.3",
    "shortid": "^2.2.8",
    "winston": "^3.1.0",
    "winston-mail": "^1.5.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.10.0",
    "mocha": "^4.0.1",
    "nodemon": "^1.18.4",
    "should": "^3.3.1",
    "supertest": "^3.0.0"
  }
}
