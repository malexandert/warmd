    "cookie-parser": "~1.3.0",
    "express": "^4.16.3",
    "express-hbs": "^2.3.0",
    "express-session": "^1.15.6",
    "forever": "^0.15.3",
    "knex": "^0.15.2",
    "lodash": "^4.17.15",
    "method-override": "^3.0.0",
    "mongoose": "^5.8.3",
